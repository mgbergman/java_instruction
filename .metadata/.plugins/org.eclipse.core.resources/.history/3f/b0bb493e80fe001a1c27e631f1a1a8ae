
public class StringsApp {

	public static void main(String[] args) {
		String firstname = "Bob";
		String lastname = "Marley";
		int age = 36;
		String message = "";
		
		message = firstname;
		message = message + " ";
		message += lastname;
		message += " was "+age+" years old when he passed.";
		sysout.out.println(message);
	}

}
